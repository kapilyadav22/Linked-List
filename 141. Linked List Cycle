class Solution {
public:
    bool hasCycle(ListNode *head) {
       if(head==NULL)
        return false;

        set<ListNode*> elements;
        while(head!=NULL)
        {    
            if(elements.count(head))
                return true;
            elements.insert(head);
             head=head->next;
        
        }
        return false;
    }
};
